{
    "version": 2,
    "builds": [
        {
            "src": "dist/server/main.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server/main.mjs"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
}